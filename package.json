{
  "name": "bot-management-system",
  "version": "1.0.21-rc5",
  "private": true,
  "scripts": {
    "ng": "ng",
    "deploy:build": "gulp deploy:build",
    "deploy:build:client": "gulp deploy:build:client",
    "build": "concurrently \"npm run build:client\" \"npm run build:server\" ",
    "build:server": "tsc -p ./server",
    "build:client": "ng build --prod --source-map=false --aot=true",
    "test:client": "ng test",
    "lint:client": "ng lint",
    "lint:server": "tslint './server/**/*.ts' -c ./server/tslint.json --fix",
    "start-dev": "concurrently \"npm run start:server\" \"npm run start:client\"",
    "start:client": "ng serve --hmr=true --proxy-config proxy.conf.json",
    "run:server": "NODE_ENV=development node dist/server/bin/www.js",
    "start:server": "npm run build:server && concurrently --kill-others \"tsc -w -p ./server\" \"npm run run:server\""
  },
